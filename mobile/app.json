{
  "expo": {
    "name": "AI Workflow",
    "slug": "ai-workflow-mobile",
    "scheme": "aiworkflow",
    "version": "0.1.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.aiworkflowmobile"
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-system-ui"
    ],
    "extra": {
      "apiUrl": "http://10.0.2.2:3001"
    }
  }
}
